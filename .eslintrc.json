{
  "env": {
    "browser": true,
    "es2021": true
  },
  // "plugins": ["import"],
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 14,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-underscore-dangle": ["error", { "allow": ["_id", "foo_"] }],
    "import/extensions": ["error", { "js": "ignorePackages" }]
  }
}
